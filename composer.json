{
    "name": "paragraph/laravel",
    "type": "library",
    "description": "Official Laravel package for Paragraph - a more productive way to manage your texts",
    "keywords": ["texts","templates","localization"],
    "homepage": "https://github.com/ParagraphApp/laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Mysenko",
            "email": "denis@paragraph.ph",
            "homepage": "https://paragraph.ph"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "5.*|^6.0|^7.0|^8.0|^9.0",
        "hidehalo/nanoid-php": "^1.1",
        "nikic/php-parser": "^4.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "larapack/dd": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Paragraph\\": "src/"
        },
        "files": [
            "helpers/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Paragraph\\Providers\\ParagraphServiceProvider"
            ]
        }
    }
}
